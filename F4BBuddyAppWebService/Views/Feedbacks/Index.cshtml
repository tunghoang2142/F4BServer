@model IEnumerable<F4BBuddyAppWebService.Models.Feedback>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Feedback1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AllStar)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Buddy)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Feedback1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AllStar.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Buddy.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
